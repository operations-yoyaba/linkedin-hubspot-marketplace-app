    "dev": "nodemon src/index.js",
    "build": "hs project build",
    "deploy": "hs project deploy",
    "watch": "hs project watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
